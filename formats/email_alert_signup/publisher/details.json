{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "summary",
    "tags",
    "format_type"
  ],
  "properties": {
    "summary": {
      "type": "string"
    },
     "tags": {
       "type": "object"
    },
    "breadcrumbs": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "link",
          "title"
        ],
        "properties": {
          "link": {
            "type": "string",
            "format": "uri"
          },
          "title": {
            "type": "string"
          }
        }
      }
    },
    "format_type": {
      "type": "string"
    }
  }
}
