{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "summary",
    "country",
    "updated_at",
    "reviewed_at",
    "change_description",
    "alert_status",
    "email_signup_link",
    "parts"
  ],
  "properties": {
    "summary": {
      "$ref": "#/definitions/multiple_content_types"
    },
    "country": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "slug",
        "name"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "slug": {
          "type": "string"
        }
      }
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "reviewed_at": {
      "type": "string",
      "format": "date-time"
    },
    "change_description": {
      "type": "string"
    },
    "alert_status": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "email_signup_link": {
      "$ref": "#/definitions/email_signup_link"
    },
    "image": {
      "$ref": "#/definitions/asset_link"
    },
    "document": {
      "$ref": "#/definitions/asset_link"
    },
    "parts": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "slug",
          "title",
          "body"
        ],
        "properties": {
          "slug": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "body": {
            "$ref": "#/definitions/body_html_and_govspeak"
          }
        }
      }
    },
    "max_cache_time": {
      "description": "The maximum length of time the content should be cached, in seconds.",
      "type": "integer"
    },
    "publishing_request_id": {
      "description": "A unique identifier used to track publishing requests to rendered content",
      "type": "string"
    }
  }
}
