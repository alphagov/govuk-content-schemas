{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
   "required": [
     "title",
     "summary",
     "content",
     "published_date"
   ],
  "properties": {
    "title": {
      "type": "string"
    },
    "summary": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "published_date": {
      "type": "string",
      "format": "date-time"
    }
  }
}
